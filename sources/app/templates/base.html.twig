{# templates/base.html.twig #}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>

    {{ encore_entry_link_tags('app') }}
</head>
<body>
    <div id="app"></div>

    {% block body %}
        {% block content %}{% endblock %}
    {% endblock %}

    {{ encore_entry_script_tags('app') }}
</body>
</html>